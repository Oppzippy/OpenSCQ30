{
  "name": "openscq30-web-react",
  "private": true,
  "version": "1.9.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:wasm && tsc && vite build",
    "build:wasm": "RUSTFLAGS=\"--cfg=web_sys_unstable_apis\" wasm-pack build --no-pack ./wasm",
    "build:wasmDev": "RUSTFLAGS=\"--cfg=web_sys_unstable_apis\" wasm-pack build --no-pack --dev ./wasm",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "e2etest": "npm run build && playwright test",
    "updateLocales": "i18next",
    "prettier:format": "prettier --write .",
    "prettier:check": "prettier --check ."
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^5.0.2",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.1",
    "@sinclair/typebox": "^0.31.1",
    "ajv": "^8.12.0",
    "dexie": "^3.2.3",
    "dexie-react-hooks": "^1.1.3",
    "i18next": "^23.2.2",
    "i18next-browser-languagedetector": "^7.0.1",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.0",
    "rxjs": "^7.8.0",
    "vite-plugin-top-level-await": "^1.3.0",
    "vite-plugin-wasm": "^3.2.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.33.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jsdom": "^21.1.1",
    "@types/lodash-es": "^4.17.7",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-test-renderer": "^18.0.0",
    "@types/web-bluetooth": "^0.0.20",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/coverage-v8": "^0.34.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "i18next-parser": "^8.0.0",
    "jsdom": "^23.0.0",
    "playwright": "^1.33.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.5.1",
    "vite-plugin-pwa": "^0.16.3",
    "vitest": "^0.34.0"
  },
  "overrides": {
    "glob-parent": "^6.0.2"
  }
}
