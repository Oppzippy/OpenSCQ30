<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="OpenSCQ30SelectedDeviceSettings" parent="GtkBox">
        <property name="orientation">vertical</property>

        <child>
            <object class="GtkNotebook" id="notebook">
                <property name="tab-pos">left</property>
                <child>
                    <object class="OpenSCQ30GeneralSettingsScreen" id="general_settings">
                        <property name="margin-start">10</property>
                        <property name="margin-end">10</property>
                        <property name="margin-top">10</property>
                        <property name="margin-bottom">10</property>
                    </object>
                </child>
                <child type="tab">
                    <object class="GtkLabel">
                        <property name="label" translatable="yes" context="general settings">General</property>
                    </object>
                </child>
                <child>
                    <object class="OpenSCQ30EqualizerSettingsScreen" id="equalizer_settings">
                        <property name="margin-start">10</property>
                        <property name="margin-end">10</property>
                        <property name="margin-top">10</property>
                        <property name="margin-bottom">10</property>
                    </object>
                </child>
                <child type="tab">
                    <object class="GtkLabel">
                        <property name="label" translatable="yes" context="equalizer settings">Equalizer</property>
                    </object>
                </child>
                <child>
                    <object class="OpenSCQ30HearIdScreen" id="hear_id">
                        <property name="margin-start">10</property>
                        <property name="margin-end">10</property>
                        <property name="margin-top">10</property>
                        <property name="margin-bottom">10</property>
                    </object>
                </child>
                <child type="tab">
                    <object class="GtkLabel">
                        <property name="label" translatable="yes" context="hear id">Hear ID</property>
                    </object>
                </child>
                <child>
                    <object class="OpenSCQ30ButtonsScreen" id="buttons">
                        <property name="margin-start">10</property>
                        <property name="margin-end">10</property>
                        <property name="margin-top">10</property>
                        <property name="margin-bottom">10</property>
                    </object>
                </child>
                <child type="tab">
                    <object class="GtkLabel">
                        <property name="label" translatable="yes" context="buttons">Buttons</property>
                    </object>
                </child>
                <child>
                    <object class="OpenSCQ30QuickPresetsScreen" id="quick_presets">
                        <property name="margin-start">10</property>
                        <property name="margin-end">10</property>
                        <property name="margin-top">10</property>
                        <property name="margin-bottom">10</property>
                    </object>
                </child>
                <child type="tab">
                    <object class="GtkLabel">
                        <property name="label" translatable="yes" context="quick presets">Quick Presets</property>
                    </object>
                </child>
                <child>
                    <object class="OpenSCQ30DeviceInformation" id="device_information">
                        <property name="margin-start">10</property>
                        <property name="margin-end">10</property>
                        <property name="margin-top">10</property>
                        <property name="margin-bottom">10</property>
                    </object>
                </child>
                <child type="tab">
                    <object class="GtkLabel">
                        <property name="label" translatable="yes" context="device information">Device Information</property>
                    </object>
                </child>
                <child>
                    <object class="OpenSCQ30ImportExport" id="import_export">
                        <property name="margin-start">10</property>
                        <property name="margin-end">10</property>
                        <property name="margin-top">10</property>
                        <property name="margin-bottom">10</property>
                    </object>
                </child>
                <child type="tab">
                    <object class="GtkLabel">
                        <property name="label" translatable="yes">Import/Export</property>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>
